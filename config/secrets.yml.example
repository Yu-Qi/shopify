# 這個檔案是 secrets.yml 的範例
# 請複製此檔案為 config/secrets.yml 並填入實際的密鑰

development:
  secret_key_base: <%= ENV.fetch("SECRET_KEY_BASE") { "your-secret-key-base-for-development" } %>

test:
  secret_key_base: <%= ENV.fetch("SECRET_KEY_BASE") { "your-secret-key-base-for-test" } %>

production:
  secret_key_base: <%= ENV["SECRET_KEY_BASE"] %>

